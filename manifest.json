{
   "background": {
      "scripts": [ "js/background.bundle.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon_toolbar.png",
      "default_popup": "browser_action.html"
   },
   "content_scripts": [ {
      "css": [ "css/youtube.css" ],
      "js": [ "js/inject.bundle.js", "js/youtube.bundle.js" ],
      "matches": [ "https://www.youtube.com/*", "http://www.youtube.com/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "",
   "homepage_url": "https://github.com/MACCNYC/no-platform-extension",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "256": "icons/iconFull.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "No Platform For Fascism Tool Kit",
   "permissions": [ "storage", "tabs", "alarms", "http://noplatform.herokuapp.com/api/*", "http://www.youtube.com/*", "https://www.youtube.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.3",
   "web_accessible_resources": [ "img/NoPlatform_small.png", "img/loading.svg" ]
}
